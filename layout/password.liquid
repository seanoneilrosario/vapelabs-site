{% layout none %}<!DOCTYPE html>
<html class="no-js full-height" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
    {% if settings.favicon %}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png" />
    {% endif %}
    {%- unless settings.type_header_font.system? -%}
    <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    {%- endunless -%}
    <title>{{ shop.name }}</title>
    <meta name="description" content="{{ page_description | escape }}">
    <!-- Social meta ================================================== -->
    {% render 'social-meta-tags' %}
    {{ content_for_header }}
    <!-- CSS ================================================== -->
    {{ 'bootstrap.min.css' | asset_url | stylesheet_tag | media:'print' }} 
    {{ 'theme.css' | asset_url | stylesheet_tag }}
    {{ 'materialdesignicons.min.css' | asset_url | stylesheet_tag }}
    {% render 'google-fonts' %}
    {% render 'css-variables' %}
    <!-- JS ================================================== -->
    {{ 'modernizr.min.js' | asset_url | script_tag }}
    {{ 'jquery.min.js' | asset_url | script_tag }}
    {{ 'bootstrap.min.js' | asset_url | script_tag }} 
    <script>document.documentElement.className = document.documentElement.className.replace('no-js', 'js');</script>
    <script src="{{ 'global.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'details-modal.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'password-modal.js' | asset_url }}" defer="defer"></script>
  </head>
  <body class="password gradient">
    <div class="page-wrapper page-element">
      <div class="container">
        <a class="skip-to-content-link button visually-hidden" href="#MainContent">
          {{ 'accessibility.skip_to_text' | t }}
        </a>
        {% section 'password-header' %}
        <main id="MainContent" class="password-main">
          {{ content_for_layout }}
        </main>
        <footer>
          {% section 'password-footer' %}
        </footer>
        {{ 'magnific-popup.min.js' | asset_url | script_tag }}
      </div>
    </div>
  </body>
</html>