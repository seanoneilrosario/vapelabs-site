<style>
  {% if page.url contains 'education-centre' %}
  {% else %}
      .footer-head_gap .gap_white,
      .footer-head_text {
        background: #77b6de !important;
      }
  {% endif %}

  main.main-content {
    padding-top: 30px !important;
  }
    
  .page-element.header_1,
  .breadcrumb-wrap {
    display: none !important;
  }

  body .page-wrapper main.main-content {
    margin-top: 0 !important;
    min-height: 100vh;
    background: #77b6de;
    padding: 40px 20px;
    transition: 0.3s;
  }

  body main.main-content,
  #shopify-section-footer-model-3,
  footer.footer {
    margin-top: 0 !important
  }

  .account-dash ul li {
    margin: 5px 10px 15px 10px;
    border-radius: 10px;
    transition: 0.3s;
  }

  .account-dash ul li:hover {
    background: #F8F8F8;
  }

  .account-dash ul li a {
    display: flex;
    align-items: center;
    font-family: "Outfit", Sans-serif;
    font-size: 18px;
    font-weight: 600;
    color: #2D4D7C;
    padding: 15px;
    line-height: 1.2;
  }

  .account-dash .icon {
    margin-right: 20px;
    display: flex;
  }

  .account-dash .icon svg {
    width: 25px;
  }

  .account-dash .icon svg path {
    stroke: #2D4D7C;
    fill: #2D4D7C;
    transition: 0.3s;
  }

  .account-dash ul li:hover .icon svg path {
    stroke: #77B6DE;
    fill: #77B6DE;
  }

  .account-dash {
    width: 300px;
    position: fixed;
    top: 0;
    left: -300px;
    min-height: 100vh;
    overflow: scroll;
    z-index: 5;
    height: 100%;
    background: #fff;
    transition: 0.3s;
  }

  .account-dash::-webkit-scrollbar {
    display: none;
  }

  .account-dash h4 {
    color: #2D4D7C;
    font-family: "Outfit", Sans-serif;
    font-size: 13px;
    font-weight: 600;
    margin: 40px 20px 10px;
  }

  .account_details {
    min-height: 100vh;
    margin-left: 300px;
    background: #2D4D7C;
  }

  .logo_toggle {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20px 20px 0;
  }

  .logo_toggle a {
    width: 140px;
  }

  .toggle_open {
    background: #2D4D7C;
    border-radius: 100%;
    width: 30px;
    height: 30px;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 26px;
    cursor: pointer;
    color: #fff;
  }

  .name_business {
    display: flex;
    align-items: center;
    margin: 20px;
    padding: 10px;
    background: #F8F8F8;
    justify-content: space-between;
    border-radius: 20px;
  }

  .name_business svg {
    background: #2D4D7C;
    width: 60px;
    height: 60px;
    padding: 13px;
    border-radius: 100px;
  }

  .name_content {
    width: calc(100% - 70px)
  }

  .name_content h3 {
    line-height: 1.2;
    color: #2d4d7c;
    font-family: "Outfit", Sans-serif;
    margin: 0;
    font-weight: 600;
  }

  .name_content span {
    color: #2d4d7c;
    font-size: 12px;
    font-weight: 500;
  }

  .account-dash.width-hide {
    left: 0;
  }

  .toggle_open {
    position: fixed;
    transition: 0.3s;
    left: 20px;
    transform: rotate(180deg);
  }

  .toggle_open.active {
    position: absolute;
    left: 250px;
    transform: rotate(0deg);
  }

  @media(min-width: 990px) {
    body .page-wrapper main.main-content {
      padding: 60px 40px;
    }

    main.main-content {
      padding-top: 60px !important;
    }
  }

  @media(min-width: 1200px) {
    body .page-wrapper main.main-content {
      margin-left: 300px !important;
    }

    body .page-wrapper main.main-content.active {
      margin-left: 0 !important;
    }

    .account-dash {
      left: 0;
    }

    .account-dash.width-hide {
      left: -300px;
    }

    .toggle_open {
      left: 250px;
      position: absolute;
      transition: 0.3s;
      transform: rotate(0deg);
    }

    .toggle_open.active {
      position: fixed;
      left: 40px;
      transform: rotate(180deg);
    }
  }

</style>
                    
