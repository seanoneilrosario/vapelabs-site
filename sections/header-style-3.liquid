                        {% when 1 %}
                        {%- assign pmenu_items_tab = 'col-sm-12' -%}
                        {% when 2 %}
                        {%- assign pmenu_items_tab = 'col-sm-6' -%}
                        {% when 3 %}
                        {%- assign pmenu_items_tab = 'col-sm-4' -%}
                        {% when 4 %}
                        {%- assign pmenu_items_tab = 'col-sm-3' -%}
                        {% when 5 %}
                        {%- assign pmenu_items_tab = 'col-sm-five' -%}
                        {% when 6 %}
                        {%- assign pmenu_items_tab = 'col-sm-2' -%}
