<style>
  .header_3_wrapper{
    background-color:{{section.settings.color_menu_bg }};
  }
  .header_3_wrapper .main-header{
    background-color:{{section.settings.main_bgheader }};
  }
  .header_3 .site-header__cart-count .cart-products-count{
    color:{{section.settings.count_color }};
    background:{{section.settings.countbgcolor }};
  }
  .header_3 .site-header__cart-count .cart-products-detail{
    color:{{section.settings.icon_txt_color }};
  }
  .header_3 .right-link-icon .myaccount .dropdown-toggle>i,.header_3 .wishlist-icon-div a>i,.header_3 .compare-icon-div a>i{
    color:{{section.settings.Header_link_color }};
  }
  .header_3 .right-link-icon .myaccount .dropdown-toggle:hover >i,.header_3 .right-link-icon .myaccount .dropdown-toggle.open >i,.header_3 .wishlist-icon-div:hover a>i,.header_3 .compare-icon-div:hover a>i{
    color:{{section.settings.Header_link_hover_color }};
  }
  .header_3 .top-header,.top_header_3 .header_language .disclosure .language_wrapper, 
  .top_header_3 a.currency_wrapper.dropdown-toggle{color:{{section.settings.top_header_text_color }};}

  .top_header_3 .header_language .disclosure .language_wrapper:hover,.top_header_3 .header_language .disclosure .language_wrapper.active, 
  .top_header_3 .currency_wrapper.dropdown-toggle:hover,.top_header_3 .currency_wrapper.dropdown-toggle.active{
    color:{{section.settings.top_header_text_hover_color }};
  }
  .header_3 .top_header_3{ background-color:{{section.settings.top_bg_color }};}
  .header_3 .bottom_header_3{ background-color:{{section.settings.bottom_bg_color }};}
  #accessibleNav>li>a{color:{{section.settings.toplink_txt_clr }};}
  #accessibleNav>li:hover>a{color:{{section.settings.toplink_txthover_clr }};}
  #accessibleNav>li:hover>a::before {background-color:{{section.settings.toplink_txtbg_clr }};}
  #tt-megamenu span.toggle_menu{ background:{{section.settings.tt_title_bgclr }}; color:{{section.settings.tt_title_clr }};}
  .tt-mega-menu #tt-megamenu .tt-mega_menu{ background:{{section.settings.menu_bgclr }};}
  #tt-megamenu .tt-mega_menu .tt_menus_ul>li>a{color:{{section.settings.menu_txt_clr }};}
  #tt-megamenu .tt_menu_item span.icon {color: {{section.settings.menu_txt_clr }};background: {{section.settings.menu_txt_clr }};}
  .tt-mega-menu #tt-megamenu .tt-mega_menu{border: 2px solid var(--color-btn-secondary); border-top: transparent}
</style>
