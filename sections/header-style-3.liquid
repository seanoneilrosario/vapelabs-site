                        {% when 3 %}
                        {%- assign pmenu_items_mob = 'col-xs-4' -%}
                        {% when 4 %}
                        {%- assign pmenu_items_mob = 'col-xs-3' -%}
                        {% when 5 %}
                        {%- assign pmenu_items_mob = 'col-lg-five' -%}
                        {% when 6 %}
                        {%- assign pmenu_items_mob = 'col-xs-2' -%}
